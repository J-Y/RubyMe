<div class="code-posts">
  <% if current_user %>
      <div class="col-md-3 text-right pull-right padding6">
        <%= link_to raw(%Q(<i class="glyphicon glyphicon-pencil"> </i>#{t(:new)})), new_code_path, class: "btn btn-primary" %>
      </div>
  <% end %>
</div>

<div class="code-posts">
  <%= render partial: 'codes', locals: {codes: @codes} %>
</div>
